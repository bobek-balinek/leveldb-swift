  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'CryptoSwift'
  
  # Pods for LevelDB-iOS
  target 'LevelDBTests-OSX' do
      pod 'Quick'
      pod 'Nimble'
  end
end
